package com.devsuperior.dsvendas.entities;

public class Seller {
	
	private Long Id;
	private String Name;
	
	public Seller () {	
	}

	public Seller(Long id, String name) {
		Id = id;
		Name = name;
	}

	public Long getId() {
		return Id;
	}

	public void setId(Long id) {
		Id = id;
	}

	public String getName() {
		return Name;
	}

	public void setName(String name) {
		Name = name;
	}
}
